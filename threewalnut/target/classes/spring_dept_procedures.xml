<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean id="p_get_dept_children" class="mhao.threewalnut.dao.StoredProcedureTemplate.MyStoredProcedure">
		<property name="jdbcTemplate" ref="jdbcTemplate" />
		<property name="sql" value="p_get_dept_children" />
		<property name="parameters">
			<list>
				<bean class="org.springframework.jdbc.core.SqlParameter">
					<constructor-arg name="name" value="i_uid" />
					<constructor-arg name="sqlType" value="1" />
				</bean>
				<bean class="org.springframework.jdbc.core.SqlParameter">
					<constructor-arg name="name" value="i_dept_pid" />
					<constructor-arg name="sqlType" value="1" />
				</bean>
				<bean class="org.springframework.jdbc.core.SqlReturnResultSet">
					<constructor-arg name="name" value="deptList" />
					<constructor-arg name="extractor">
						<ref bean="rsExtractor"/>
					</constructor-arg>
				</bean>
				<bean class="org.springframework.jdbc.core.SqlReturnResultSet">
					<constructor-arg name="name" value="positionList" />
					<constructor-arg name="extractor">
						<ref bean="rsExtractor"/>
					</constructor-arg>
				</bean>
				<bean class="org.springframework.jdbc.core.SqlReturnResultSet">
					<constructor-arg name="name" value="pageInfo" />
					<constructor-arg name="extractor">
						<ref bean="rsExtractor"/>
					</constructor-arg>
				</bean>
			</list>
		</property>
	</bean>
	
